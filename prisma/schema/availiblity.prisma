model AvailabilitySlot {
  id      String       @id @default(cuid())
  tutorId String
  tutor   TutorProfile @relation(fields: [tutorId], references: [userId], onDelete: Cascade)

  startTime DateTime
  endTime   DateTime
  isBooked  Boolean  @default(false)

  booking Booking?

  createdAt DateTime @default(now())
}
